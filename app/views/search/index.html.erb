<%= form_tag(slots_search_index_path, id: "search_free_slots_form", remote: true) do %>
  <!--Services <br/>
  <%#= select_tag("search[service]",  options_for_select(@services)) %><br/>-->
  Date <br/>
  <%= text_field_tag "search[date]", nil,class: "date-pick", readonly: true %> <br/><br/><br/>
  Time <br/>
  <%= text_field_tag "search[time]", nil,class: "time_entry" %><%= image_tag("icon_clock.png") %><br/>
  <%= link_to "Search", "javascript:void(0)", class: 'btn btn-primary search-free-slots' %>
<% end %>
<div id="slots">
</div>
<script>
  $(".time_entry").timeEntry({show24Hours: true});
  $('.date-pick').datePicker();
</script>